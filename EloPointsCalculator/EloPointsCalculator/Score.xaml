<Window
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:local="clr-namespace:EloPointsCalculator"
        xmlns:System="clr-namespace:System;assembly=mscorlib" x:Class="EloPointsCalculator.Score"
        mc:Ignorable="d"
        Title="Score" Height="249" Width="173">
    <Grid>
        <TextBox x:Name="Player1" HorizontalAlignment="Left" Height="17" Margin="75,80,0,0" TextWrapping="Wrap" VerticalAlignment="Top" Width="80"/>
        <TextBox x:Name="Player2" HorizontalAlignment="Left" Height="17" Margin="75,102,0,0" TextWrapping="Wrap" VerticalAlignment="Top" Width="80"/>
        <Button x:Name="SubmitScore" Content="Submit" HorizontalAlignment="Left" Height="44" Margin="42,142,0,0" VerticalAlignment="Top" Width="80" Click="SubmitScore_Click"/>
        <TextBlock HorizontalAlignment="Left" Height="17" Margin="10,80,0,0" TextWrapping="Wrap" Text="Winner" VerticalAlignment="Top" Width="60"/>
        <TextBlock HorizontalAlignment="Left" Height="17" Margin="10,102,0,0" TextWrapping="Wrap" Text="Loser" VerticalAlignment="Top" Width="60"/>
    </Grid>
</Window>
